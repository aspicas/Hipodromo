update carrera set carr_nombre='nuevo',carr_categoria='copa',carr_fecha=to_date('2015-02-15','yyyy-mm-dd'),carr_num_carrera=4,carr_lote='5df45df',carr_distancia=2,carr_variante=5 where carr_codigo = 13;

fk_pist=114,fk_hora=(select hora_codigo from hora where hora_inicio='15:00:00' and hora_dia='viernes')
