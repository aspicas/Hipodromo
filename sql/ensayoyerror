update carrera set carr_nombre='nuevo',carr_categoria='copa',carr_fecha=to_date('2015-02-15','yyyy-mm-dd'),carr_num_carrera=4,carr_lote='5df45df',carr_distancia=2,carr_variante=5 where carr_codigo = 13;

fk_pist=114,fk_hora=(select hora_codigo from hora where hora_inicio='15:00:00' and hora_dia='viernes')

update stud set stud_nombre='pow',stud_fecha_creacion=to_date('2015-01-01','yyyy-mm-dd') where stud_codigo=105;

update ejemplar set ejem_nombre='nombre',ejem_pelaje='pel',ejem_fecha_nac=to_date('2015-01-01','yyyy-mm-dd'),ejem_sexo='macho',ejem_tatuaje='213wqe',ejem_raza=null,fk_ejem_m=null,fk_ejem_p=null,fk_hara=5,fk_pues=10 where ejem_codigo=26;
