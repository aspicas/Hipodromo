select apue_codigo,apue_nombre,apue_minimo,apue_regla from apuesta order by apue_codigo;

update apuesta set apue_nombre='lala',apue_minimo='100',apue_regla='Bachaquear' where apue_codigo=26;

update caej set caej_puesto_partida=2,caej_num_ejemplar=2,caej_posicion=2,caej_velocidad=2,caej_cuerpo=2,caej_retiro=null,caej_num_llamado=2,caej_fecha_ins=to_date('2004-02-02','yyyy-mm-dd'),caej_monto_ins=22,caej_fecha_limite_ins=to_date('2004-03-02','yyyy-mm-dd'),caej_favorito=null,fk_ejem=28,fk_carr=14,fk_hora=71,fk_jine=5 where caej_codigo=1;


select * from product_component_version;

select usua_codigo from usuario where usua_nombre='admin' and usua_clave='admin';

select rol_permiso from usuario,rol,rous where fk_usua=usua_codigo and fk_rol=rol_codigo and usua_nombre='admin';

select fk_prop from usuario where usua_nombre='lali';

/*Listado de Usuario con sus Roles*/
select usua_nombre as Usuario,rol_nombre as Rol from usuario,rous,rol where fk_usua=usua_codigo and fk_rol=rol_codigo;


