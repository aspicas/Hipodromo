select trunc(months_between(sysdate, to_date(clie_fecha_nacimiento,'dd/mm/yyyy'))/12) from clientes  

SELECT trunc((SYSDATE - to_date('08/08/1985','dd/mm/rrrr'))/365,0) FROM dual

SELECT trunc((SYSDATE - to_date(ejem_fecha_nac,'dd/mm/rrrr'))/365,0), ejem_nombre FROM ejemplar;

select to_char(C.carr_fecha,'dd-mm-yyyy') Fecha,H.hora_inicio Hora,H.hora_dia Dia,CA.CAEJ_NUM_LLAMADO NL,C.carr_tipo Tipo,carr_distancia Distancia,C.carr_num_carrera NC, SUM(P.prem_monto) Premio,C.carr_nombre Carrera,C.carr_categoria Categoria,
CA.caej_num_ejemplar NE,E.ejem_nombre Ejemplar, stud_nombre Stud, E.ejem_pelaje Pel, trunc((SYSDATE - to_date(E.ejem_fecha_nac,'dd/mm/rrrr'))/365,0) Edad,jine_nombre ||' '|| jine_apellido Jinete,
PEJ.peso_peso PesoJ,caej_puesto_partida PP,E.ejem_sexo Sexo,PA.ejem_nombre Padre,MA.ejem_nombre Madre, entr_nombre ||' '|| entr_apellido Entrenador
from carrera C,caej CA,ejemplar E, hora H, premio P, PRST PR, stud S,jinete j,peso PEJ,ejemplar PA,ejemplar MA, puesto PUE, caballeriza cab, entrenador
where CA.fk_carr=carr_codigo and CA.fk_ejem=E.ejem_codigo and P.fk_carr=carr_codigo and PR.fk_ejem=E.ejem_codigo and PR.fk_stud=stud_codigo and CA.fk_jine=jine_codigo
and PEJ.fk_caej=caej_codigo and E.fk_ejem_p=PA.ejem_codigo and E.fk_ejem_m=MA.ejem_codigo and E.fk_pues=pues_codigo and PUE.fk_caba=caba_codigo and cab.fk_entr=entr_codigo
and hora_inicio='12:30:00' and hora_dia='sabado' and carr_nombre='Carrera2' and PEJ.peso_tipo='jinete'
group by carr_fecha,hora_inicio,hora_dia,caej_num_llamado,carr_tipo,carr_distancia,carr_num_carrera,carr_nombre,carr_categoria,caej_num_ejemplar,E.ejem_nombre,stud_nombre,
E.ejem_pelaje,trunc((SYSDATE - to_date(E.ejem_fecha_nac,'dd/mm/rrrr'))/365,0),jine_nombre ||' '|| jine_apellido ,PEJ.peso_peso,caej_puesto_partida,E.ejem_sexo,PA.ejem_nombre,
MA.ejem_nombre,entr_nombre ||' '|| entr_apellido
order by NE;


select to_char(carr_fecha,'dd/mm/yyyy') Fecha,hora_inicio Hora,hora_dia Dia,carr_num_carrera NC,carr_distancia Dist, SUM(prem_monto) Premio, carr_tipo Tipo, carr_lote
from carrera C, hora H, premio P
where C.fk_hora=hora_codigo and carr_nombre='Carrera1'
group by carr_fecha,hora_inicio,hora_dia,carr_num_carrera,carr_distancia,carr_tipo,carr_lote;

select CA.caej_puesto_partida PP, CA.caej_num_ejemplar NE, PA.ejem_nombre Padre, MA.ejem_nombre Madre, E.ejem_sexo Sexo, E.ejem_pelaje Pelaje, entr_nombre ||' '|| entr_apellido Entrenador,
S.stud_nombre Stud, E.ejem_nombre Ejemplar, jine_nombre ||' '|| jine_apellido Jinete, PEJ.peso_peso PJINE
from carrera C, caej CA, ejemplar E, ejemplar PA, ejemplar MA, puesto P, caballeriza CAB, entrenador EN, PRST PR, stud S, jinete J, peso PEJ
where CA.fk_carr=C.carr_codigo and CA.fk_ejem=E.ejem_codigo and E.fk_ejem_p=PA.ejem_codigo and E.fk_ejem_m=MA.ejem_codigo
and E.fk_pues=P.pues_codigo and P.fk_caba=CAB.caba_codigo and CAB.fk_entr=EN.entr_codigo and PR.fk_ejem=E.ejem_codigo and PR.fk_stud=stud_codigo and CA.fk_jine=J.jine_codigo and PEJ.fk_caej=CA.caej_codigo
and PEJ.peso_tipo='jinete' and carr_nombre='Carrera2'
group by E.ejem_nombre,CA.caej_puesto_partida, CA.caej_num_ejemplar, PA.ejem_nombre, MA.ejem_nombre, E.ejem_sexo, E.ejem_pelaje, entr_nombre ||' '|| entr_apellido,
S.stud_nombre, jine_nombre ||' '|| jine_apellido,PEJ.peso_peso;
